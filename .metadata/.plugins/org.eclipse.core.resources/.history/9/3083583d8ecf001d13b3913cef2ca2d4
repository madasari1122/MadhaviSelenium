import java.util.HashMap;

public class hashMapExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		HashMap<Integer,String> hm= new HashMap<Integer,String>();
		hm.put(0, "hello");
		hm.put(1, "good bye");
		hm.put(42,"morning");
		hm.put(3,"evening");
		System.out.println(hm.get(42));
		hm.remove(42);
		System.out.println(hm.get(42));
			
		
	}

}
